####################################################################
### KAFKA ENV - override values with env/override.expo-kafka.env ###
####################################################################

KAFKA_ENABLED={{ expo_kafka_enabled | upper }}
KAFKA_EXTERNAL={{ expo_kafka_external | upper }}
KAFKA_USERNAME="{{ expo_kafka_username }}"
