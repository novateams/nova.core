####################################################################
### KAFKA ENV - override values with env/override.expo-kafka.env ###
####################################################################

KAFKA_ENABLED={{ expo_kafka_enabled | upper }}
KAFKA_EXTERNAL={{ expo_kafka_external | upper }}
KAFKA_USERNAME="{{ expo_kafka_username }}"
KAFKA_PASSWORD="{{ expo_kafka_password }}"

KAFKA_KSQL_ENABLED={{ expo_kafka_ksql_enabled | upper }}
KAFKA_KSQL_USERNAME="{{ expo_kafka_ksql_username }}"
KAFKA_KSQL_PASSWORD="{{ expo_kafka_ksql_password }}"

TRINO_ENABLED={{ expo_trino_enabled | upper }}
TRINO_URL={{ expo_trino_url }}
TRINO_CATALOG={{ expo_trino_catalog }}
TRINO_SCHEMA={{ expo_trino_schema }}
TRINO_USERNAME="{{ expo_trino_username }}"
TRINO_PASSWORD="{{ expo_trino_password }}"